</classpath>
</taskdef>

<installPackage namespace="clcommon" version="3.0000" username="${sf.username}" password="${sf.password}" serverurl="${sf.serverurl}" />
<installPackage namespace="mfiflexUtil" version="4.2000" username="${sf.username}" password="${sf.password}" serverurl="${sf.serverurl}" />
<installPackage namespace="integration framework" version="3.0000" username="${sf.username}" password="${sf.password}" serverurl="${sf.serverurl}" />	
